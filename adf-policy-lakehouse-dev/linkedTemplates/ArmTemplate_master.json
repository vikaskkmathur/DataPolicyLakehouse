{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-policy-lakehouse-dev"
		},
		"ls_adb_pol_lake_dev_mi_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0214-051314-4ekorfs2"
		},
		"ls_adls2_pol_lake_dev_mi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stpolicylakevmdev.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-policy-lakehouse-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adb_pol_lake_dev_mi_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_adb_pol_lake_dev_mi_properties_typeProperties_existingClusterId')]"
					},
					"ls_adls2_pol_lake_dev_mi_properties_typeProperties_url": {
						"value": "[parameters('ls_adls2_pol_lake_dev_mi_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}